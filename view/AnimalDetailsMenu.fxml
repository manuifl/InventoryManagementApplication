<AnchorPane id="AnchorPane" prefHeight="340.0" prefWidth="360.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AnimalDetailsMenuController">
   <children>
      <VBox prefHeight="350.0" prefWidth="360.0">
         <children>
            <GridPane vgap="30.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="175.0" minWidth="10.0" prefWidth="71.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="331.0" minWidth="10.0" prefWidth="289.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="ID:" />
                  <Label text="Breed:" GridPane.rowIndex="1" />
                  <Label text="Lifespan:" GridPane.rowIndex="2" />
                  <Label text="Behaviour" GridPane.rowIndex="3" />
                  <Label text="Price:" GridPane.rowIndex="4" />
                  <Label text="Vaccinated:" GridPane.rowIndex="5" />
                  <Label text="Special:" GridPane.rowIndex="6" />
                  <Label fx:id="animalIdLbl" text="Label" GridPane.columnIndex="1" />
                  <Label fx:id="breedLbl" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Label fx:id="lifespanLbl" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <Label fx:id="behaviourLbl" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <Label fx:id="priceLbl" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                  <Label fx:id="vacLbl" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                  <Label fx:id="specialLbl" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="6" />
               </children>
            </GridPane>
            <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#onActionDisplayMainMenu" text="Back" />
               </children>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </HBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="20.0" />
         </padding>
      </VBox>
